{
  "manifest_version": 2,
  "name": "Getting started example",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://www.okex.com/*"
      ],
      "js": [
        "/js/grab-cake.js"
      ],
      "runat":"document_end"
    }
  ],
  "background":{
   
    "page": "background.html"
},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"

  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}